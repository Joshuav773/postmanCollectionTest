						"exec": [
							"var jsonData = pm.response.json();\r",
							"var sections = request.data.sectionNumbers; \r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200); \r",
							"});\r",
							"\r",
							"pm.test(\"scraper started at section expected\", function () {\r",
							"    //no way of telling which section it started from with the current the response\r",
							"    var firstSection = sections[0];\r",
							"    pm.expect(firstSection).to.eq('05-00-01')\r",
							"    //from the response, we should get a list of the sections scraped for clarity.\r",
							"    //after the refactor we should be able to match what we sent vs what was actually done.\r",
							"});\r",
							"\r",
							"pm.test(\"scraper finished at section expected\", function () {\r",
							"    //no way of telling which section it started from with the current the response\r",
							"    var lastSection = sections[sections.length - 1];\r",
							"    pm.expect(firstSection).to.eq('05-00-02')\r",
							"    //from the response, we should get a list of the sections scraped for clarity.\r",
							"    //after the refactor we should be able to match what we sent vs what was actually done.\r",
							"});\r",
							"\r",
							"pm.test(\"Scraped sections specified\", () => {\r",
							"    //should create a template for each section scraped.\r",
							"    var count = sections.length;\r",
							"    pm.expect(jsonData).to.have.lengthOf(count);\r",
							" });"
